    "import re\n",
    "text = \"\"\"\n",
    "    99405 55241\n",
    "    7878943499\n",
    "    (027)324-3232\n",
    "\"\"\"\n",
    "digits10=r'\\d{10}'\n",
    "digits55=r'\\d{5}\\D\\d{5}'\n",
    "digitsus=r'\\(\\d{3}\\)\\s?\\d{3}\\D\\d{4}'\n",
    "\n",
    "regex=digits10+'|'+digits55+'|'+digitsus\n",
    "mobiles=re.findall(regex,text)\n",
    "print(mobiles)\n"
   ]
